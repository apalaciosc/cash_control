
<%-# <%= render "devise/shared/links"% > -%>


<%= form_for(resource, as: resource_name, url: session_path(resource_name), :html => {class:'md-float-material form-material'})  do |f| %>

<div class="auth-box card">
  <div class="card-block">
    <div class="row m-b-20">
      <div class="col-md-12">
        <h3 class="text-center">
          Login
        </h3>
      </div>
    </div>
    <div class="form-group form-primary">
     <%= f.email_field :email,class:'form-control' %>


     <span class="form-bar">
     </span>
     <label class="float-label">
      Ingrese email
    </label>

  </input>
</div>
<div class="form-group form-primary">

 <%=f.password_field :password, autocomplete: "current-password",class:'form-control' %>
  <span class="form-bar">
  </span>
  <label class="float-label">
    Contraseña
  </label>

</div>
<div class="row m-t-25 text-left">
  <div class="col-12">
    <div class="checkbox-fade fade-in-primary d-">
      <label>
        <% if devise_mapping.rememberable? -%>
       <%= f.check_box :remember_me %>
        <span class="cr">
          <i class="cr-icon icofont icofont-ui-check txt-primary">
          </i>
        </span>
        <% end -%>
        <span class="text-inverse">
          Recordarme
        </span>
      </input>
    </label>
  </div>
  <!-- <div class="forgot-phone text-right float-right">
    <a class="text-right f-w-600" href="auth-reset-password.html">
      ¿Olvido su clave?
    </a>
  </div> -->
</div>
</div>
<div class="row m-t-30">
  <div class="col-md-12">
     <%= f.submit "Ingresar", class:'btn btn-primary btn-md btn-block waves-effect waves-light text-center m-b-20' %>
    
     
  </div>
</div>
<hr/>

</div>
</div>
<% end %>