

<%= render "devise/shared/links" %>

<div class="card-block">

<%= form_for(resource, as: resource_name, url: registration_path(resource_name), :html => {class:'md-float-material form-material'})  do |f| %>
 <%= devise_error_messages! %>

<div class="auth-box card">
  <div class="card-block">
    <div class="row m-b-20">
      <div class="col-md-12">
        <h3 class="text-center">
          Login
        </h3>
      </div>
    </div>
    <div class="form-group form-primary">
     <%= f.text_field :name, autofocus: true, autocomplete: "name",class:'form-control' %>


     <span class="form-bar">
     </span>
     <label class="float-label">
      Ingrese nombre
    </label>

  </input>
</div>
 <div class="form-group form-primary">
     <%= f.text_field :address, autofocus: true, autocomplete: "address",class:'form-control' %>


     <span class="form-bar">
     </span>
     <label class="float-label">
      Ingrese dirección
    </label>

  </input>
</div>
<div class="form-group form-primary">
     <%= f.text_field :phone, autofocus: true, autocomplete: "phone",class:'form-control' %>


     <span class="form-bar">
     </span>
     <label class="float-label">
      Ingrese telefono
    </label>

  </input>
</div>
    <div class="form-group form-primary">
     <%= f.email_field :email, autofocus: true, autocomplete: "email",class:'form-control' %>


     <span class="form-bar">
     </span>
     <label class="float-label">
      Ingrese email
    </label>

  </input>
</div>
<div class="form-group form-primary">

 <%=f.password_field :password, autocomplete: "new-password",class:'form-control' %>
  <span class="form-bar">
  </span>
  <label class="float-label">
    Contraseña
  </label>

</div>
<div class="form-group form-primary">

 <%=f.password_field :password_confirmation, autocomplete: "new-password",class:'form-control' %>
  <span class="form-bar">
  </span>
  <label class="float-label">
    Confirmar contraseña
  </label>

</div>

  <!-- <div class="forgot-phone text-right float-right">
    <a class="text-right f-w-600" href="auth-reset-password.html">
      ¿Olvido su clave?
    </a>
  </div> -->
</div>
</div>
<div class="row m-t-30">
  <div class="col-md-12">
     <%= f.submit "Registar", class:'btn btn-primary btn-md btn-block waves-effect waves-light text-center m-b-20' %>
    
     
  </div>
</div>
<hr/>

</div>
</div>
<% end %>
</div>